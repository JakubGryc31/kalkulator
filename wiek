#include <iostream>
using namespace std;
int wiek;

int main()
{
    std::cout << "ile masz lat?";
    cin >> wiek;
    if (wiek < 18)
    {
        cout << "nie jestes pelnoletni i nie mozesz kandydowac" << endl;
    }
    else if ((wiek>=18)&&(wiek<35))
    {
        cout << "jestes pelnoletni, ale nie mozesz kandydowac";
    }
    else
    {
        cout << "jestes pelnoletni i mozesz kandydowac";
    }
    return 0;
}
